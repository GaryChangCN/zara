{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "only-arrow-functions": [
            false
        ],
        "semicolon": [
            false, "always"
        ],
        "space-before-function-paren": false,
        "quotemark": [
            true, "single"
        ],
        "no-empty": false
    },
    "rulesDirectory": []
}